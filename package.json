{
	"name": "angularjs-template-autocomplete",
	"displayName": "AngularJS Template Autocomplete",
	"description": "Auto-complete angularjs controller properties and methods from an html template",
	"version": "1.0.0",
	"icon": "angularjs-128.png",
	"publisher": "michaelisom",
	"repository": {
		"type": "git",
		"url": "https://github.com/Airizom/angularjs-template-autocomplete.git"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.5.2",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"tsutils": "^3.14.0"
	}
}